          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background-color": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".underline": {
              "padding-top": ".5rem",
              ".line": {
                "width": "5rem",
                "height": "1px",
                "display": "inline-block",
                "background-color": "@underlineColor",
                "transition": "width .3s"
              }
            },
            ".title-wrap:hover": {
              ".line": {
                "width": "6rem"
              }
            },
            "& when (@showTitle), (@showSubtitle)": {
              ".title-wrap": {
                "padding-bottom": "3rem"
              }
            },
            "& when (@showTitle) and (@showSubtitle)": {
              ".mbr-section-subtitle": {
                "padding-top": "1rem"
              }
            },
            "thead": {
              "& when not (@transparentTable)": {
                "background": "@headerColor"
              }
            },
            "table": {
              "width": "100%",
              "border-collapse": "collapse",
              "min-width": "500px",
              "td": {
                "border-top": "none",
                "padding": ".75rem",
                "& when not (@stripedTable), (@transparentTable)": {
                  "border-bottom": "1px solid @borderColor"
                }
              },
              "th": {
                "border-top": "none",
                "padding": ".75rem",
                "border-bottom": "1px solid @borderColor"
              }
            },
            "tbody tr": {
              "& when not (@transparentTable)": {
                "background": "@bodyColor",
                "& when (@stripedTable)": {
                  "&:nth-child(even)": {
                    "background": "@bodyColor2"
                  }
                }
              }
            },
            "p": {
              "padding": "5px",
              "margin": "0px"
            },
            ".scroll": {
              "overflow-x": "auto"
            },
            ".wrapscroll": {
              "width": "100%"
            }
